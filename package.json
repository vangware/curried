{
  "name": "@vangware/curried",
  "description": "üçõ Vangware curried vanilla aliases",
  "version": "1.0.0",
  "author": {
    "name": "Vangware",
    "url": "https://vangware.com"
  },
  "bugs": {
    "url": "https://github.com/vangware/curried/issues"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/node": "^14.14.21",
    "@types/prettier": "^2.1.6",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "@vangware/base": "^4.2.4",
    "@vangware/test": "^5.0.5",
    "c8": "^7.4.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-ban": "^1.5.2",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.20.16",
    "typescript": "^4.1.3"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./types": {
      "import": "./esm/types/index.js",
      "require": "./cjs/types/index.js"
    }
  },
  "files": [
    "cjs",
    "esm"
  ],
  "homepage": "https://curried.vangware.com",
  "husky": {
    "hooks": {
      "pre-commit": "run-s clean pre-compile lint test docs"
    }
  },
  "keywords": [
    "curried",
    "curry",
    "deno",
    "esm",
    "fp",
    "functional",
    "node",
    "pure",
    "types",
    "typescript",
    "utils",
    "vangware",
    "vanilla"
  ],
  "license": "MIT",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vangware/curried.git"
  },
  "scripts": {
    "clean": "rimraf ./{cjs,esm}",
    "compile": "run-s compile:*",
    "compile:cjs": "tsc --project tsconfig.cjs.json",
    "compile:esm": "tsc --project tsconfig.esm.json",
    "docs": "typedoc --gitRevision main --theme minimal ./src/index.ts",
    "lint": "eslint {src,tests}/**/*.ts",
    "postpublish": "run-s clean",
    "pre-compile": "run-s pre-compile:*",
    "pre-compile:cjs": "tsc --noemit --project tsconfig.cjs.json",
    "pre-compile:esm": "tsc --noemit --project tsconfig.esm.json",
    "prepublishOnly": "run-s compile prettify",
    "prettify": "prettier --write './{cjs,esm}/**/*.{js,ts}'",
    "test": "c8 --reporter=html --reporter=text-summary --reporter=lcov test"
  },
  "source": "./src/index.ts",
  "types": "./esm/index.d.ts"
}
